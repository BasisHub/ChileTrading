SELECT CUSTOMER.COMPANY AS COMPANY, CUSTOMER.FIRST_NAME AS FIRST_NAME, INVOICE.INVOICE_NUM AS INVOICE, CUSTOMER.LAST_NAME AS LAST_NAME, CUSTOMER.PHONE AS PHONE, INVOICE.TOTAL_AMOUNT AS TOTAL FROM CUSTOMER CUSTOMER, INVOICE INVOICE WHERE (CUSTOMER.CUST_NUM=INVOICE.CUST_NUM AND INVOICE.PAID_AMOUNT<INVOICE.TOTAL_AMOUNT)